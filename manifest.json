{
  "manifest_version": 2,

  "name": "Bittex salmon phishing",
  "description": "Bittex salmon phishing",
  "version": "1.0",
  "icons": {
    "16" : "salmon-16.png",
    "48" : "salmon-48.png",
    "64" : "salmon-64.png",
    "128" : "salmon-128.png",
    "256" : "salmon-256.png",
    "512" : "salmon-512.png"
  },
  "browser_action": {
    "default_icon": {
      "16" : "salmon-16.png",
      "48" : "salmon-48.png",
      "64" : "salmon-64.png",
      "128" : "salmon-128.png",
      "256" : "salmon-256.png",
      "512" : "salmon-512.png"  
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://upbit.com/",
    "https://api-manager.upbit.com/",
    "https://www.bittrex.com/",
    "https://www.binance.com/api/",
    "https://api.binance.com/api/",
    "https://api.hitbtc.com/api/",
    "https://api.etherdelta.com/",
    "https://ajax.googleapis.com/",
    "activeTab",
    "https://www.bittrex.com/api/",
    "https://socket.bittrex.com/signalr",
    "wss://socket.bittrex.com/",
    "notifications",
    "storage",
    "management"
  ], 
  "background": {
    "scripts": ["jquery-3.2.1.min.js", "jquery.number.js", "jquery.signalR.js", "hubs.js", "notifyqueue.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.bittrex.com/Market/Index?MarketName=BTC-*"],
      "js": ["content_scripts.js"]
    }
  ],
  "options_page": "options.html"
}